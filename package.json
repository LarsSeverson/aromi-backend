{
  "type": "commonjs",
  "name": "aromi-backend",
  "version": "1.0.0",
  "description": "Backend to aromi",
  "author": "aromi-dev",
  "scripts": {
    "dev": "sudo sls offline --host 0.0.0.0",
    "start": "npm run build && node dist/src/server.js",
    "build": "tsc && tsc-alias && copyfiles -u 2 src/generated/schema.graphql dist/src/generated",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "codegen": "graphql-codegen --config src/scripts/codegen.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@as-integrations/aws-lambda": "^3.1.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.24.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "pg": "^8.13.3",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.5",
    "@graphql-codegen/typescript-resolvers": "^4.4.4",
    "@types/graphql-fields": "^1.3.9",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/pg": "^8.11.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "serverless-offline": "^14.4.0",
    "standard": "^17.1.2",
    "tsc-alias": "^1.8.11",
    "typescript": "^5.7.3"
  }
}
