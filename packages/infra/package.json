{
  "name": "@aromi/infra",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "author": "Lars Severson",
  "license": "MIT",
  "scripts": {
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap --profile aromi-prod",
    "cdk:synth": "cdk synth --profile aromi-prod",
    "deploy:base": "cdk deploy \"*-global\" \"*-foundation\" \"*-identity\" \"*-dns\" \"*-data\" --profile aromi-prod --require-approval never",
    "deploy:app": "cdk deploy \"*-application\" \"*-edge\" --profile aromi-prod --require-approval never",
    "deploy:all": "cdk deploy --all --profile aromi-prod --requirs-approval never",
    "cdk:destroy": "cdk destroy --all"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.230.0",
    "constructs": "^10.4.3"
  },
  "devDependencies": {
    "aws-cdk": "^2.1033.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}